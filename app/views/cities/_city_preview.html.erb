<div class="item_float">
  <div class="place-found">
    <div class="place-photo">
      <%=h link_to image_tag(place_small_photo(city), :class => ""), city_path(city) %><br/>
    </div>
    <%#*<div class="overlayinfo">%>
      <div class="place-desc">
        <div class="general_box">
          <div class="break-word shorten_no_link medium_text">
            <%=h city.name %>
          </div>
          <%# if !place.category.nil? %>
            <%#*<div class="general_box">%>
              <%#=h place.category.name %>
            <%#*</div>%>
          <%# end %>
          <span class="right_div"><%= link_to fa_icon("map-marker", :class => "cyancolor mediumsize"), "", :id => "show_on_map", :'data-index' => index, :class => "general show_on_map hastip", :title => "Show on map" %></span>
          <%#*<span>%>
            <%#= fa_icon("comment", :class => "cyancolor mediumsize hastip", :title => "Review") %>
            <%#= place.reviews.count %>
          <%#*</span>%>
          <%#*<span>%>
            <%#= fa_icon("heart", :class => "cyancolor mediumsize hastip", :title => "Like") %>
            <%#= place.favorite_places_count %>
          <%#*</span>%>
          <%#*<span>%>
            <%#= fa_icon("user", :class => "cyancolor mediumsize hastip", :title => "Visitor") %>
            <%#= place.visited_places_count %>
          <%#*</span>%>
        </div>
      </div>
    </div>

    <%#= render :partial => "shared/place_small_info_box", :locals => { :place => place } %>
  </div>
<%#*</div>%>