<% content_for :title, Footprint32::APP_NAME %>
<div class="page-container">
  <div id="content">
    <%#*<div class="padding">%>
      <div class="resource_container">
        <div class="title_text"><%= t('.title')%></div>
      </div>
      <div class="resource_container">
        <div id="photo_partial">
          <%= render :partial => 'cover_photo/photos', :locals => {:photos => @photos} %>
        </div>
        <div id="paginator_photo">
          <%= paginate @photos, :remote => true, :param_name => "photo_page", :theme => "default" %>
        </div>
      </div>

    <%#*</div>%>
  </div>
</div>
