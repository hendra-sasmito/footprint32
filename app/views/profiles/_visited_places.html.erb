<div class="info-section-title title_text"><%= link_to t('.title'), user_visited_places_path(user), :class => "general hastip", :title => "Show on map" %> (<%= user.visited_places.count %>) </div>
<div class="info-section-content">
<% visited_places.each do |visited_place| %>
  <div class="hidden_box_container left_menu">
    <%= render :partial => "profiles/place_hover_info", :locals => {:place => visited_place.place, :type => "vp", :id => visited_place.id} %>
  </div>
<% end %>
<div class="clear_float"></div>
</div>