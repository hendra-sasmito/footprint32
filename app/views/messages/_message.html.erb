<td>
  <% message_status.message.users.each do |user| %>
    <% if user == current_user %>
    <% else %>
      <%= link_to image_tag(get_profile_photo_url(user.profile_photo),
        :class => "thumb_profile_pic imagedropshadow"), user_profile_path(user), :class => "hastip", :title => "#{user.profile.full_name}" %>
    <% end %>
  <% end %>
</td>
<td><%= link_to message_status.message.subject, message_path(message_status.message), :class => "general" %></td>
<td>
  <%= link_to fa_icon("trash-o", :class => "cyancolor mediumsize"), message_path(message_status.message), :confirm => 'Are you sure?', :method => :delete, :class => "hastip", :title => "Delete" %>
  <%= link_to fa_icon("file-o", :class => "cyancolor mediumsize"), unread_message_path(message_status.message), :method => :post, :class => "hastip", :title => "Mark as unread" %><br />
</td>