<div class="item_float">
  <div class="nearby-place-found">
    <div class="nearby-place-image">
      <%=h link_to image_tag(get_small_photo_url(place.default_photo), :class => "tiny_image"), place_path(place) %><br/>
    </div>
    <div class="nearby-place-content small_text">
      <div class="break-word nearby-place-name">
        <%=h link_to place.name, place_path(place), :class => "general" %>
      </div>
      <div class="">
        <%= link_to fa_icon("map-marker", :class => "graycolor mediumsize"), "", :id => "show_on_map", :'data-index' => index, :class => "general show_on_map hastip", :title => "Show on map" %>  <%= place.street if place.street %>
      </div>
    </div>
    <div class="clear_float"></div>
  </div>
</div>
