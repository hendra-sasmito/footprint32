<div id="mygallery" >
  <div class="container">
    <% if !place_photos.nil? %>

      <% place_photos.each do |photo| %>
        <div class="photo left_div">
          <%= link_to image_tag(photo.image.url(:small), :class => "curvedframe"), photo.image.url(:original),
            :class=>"fancybox-buttons", "data-fancybox-group"=>"place" %>
        </div>
      <% end %>
    <% end %>
  </div>
</div>
<% if !place_photos.nil? %>
  <div id="photo_description" style="display: none;">
    <% place_photos.each do |photo| %>
      <%= render :partial => "photos/photo_description", :locals => {:photo => photo} %>
    <% end %>
  </div>
<% end %>