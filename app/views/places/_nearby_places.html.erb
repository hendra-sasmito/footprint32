<div class="info-section-title title_text"><%= t('.title')%></div>
<div class="info-section-content">
  <% index = 0 %>
  <% nearby_places.each do |nearby_place| %>
    <%= render :partial => "places/nearby_place_preview", :locals => {:place => nearby_place, :index => index} %>
    <% index = index + 1 %>
  <% end %>
<div class="clear_float"></div>
</div>
<script id="nearby_places" type="text/json">
  <%= raw places_list.to_json %>
</script>
