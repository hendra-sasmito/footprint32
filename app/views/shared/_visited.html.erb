<div class="info-section-title title_text">Visitor</div>
<div class="info-section-content">
  <div class="general_box">
    <% visited.each do |v| %>
      <div class="hidden_box_container left_menu">
        <%=h link_to image_tag(get_profile_photo_url(v.user.profile_photo),
            :class => "small_pic "), user_profile_path(v.user), :class => "hastip", :title => "#{v.user.profile.full_name}" %>
        <%#= render :partial => "shared/user_small_info_box", :locals => { :user => v.user } %>
      </div>
    <% end %>
  </div>
</div>
<div class="clear_float"></div>

