<% content_for :title, Footprint32::APP_NAME %>
<div class="page-container">
  <div id="content">
    <div class="padding">
      <div class="resource_container_blue">
        <div class="title_text"><%= t('.title')%> <span class="wordwrap"><%= @photo_album.name %></span></div>
      </div>
      <div class="resource_container">
        <%= render 'form' %>
      </div>
      <div class="resource_container">
        <%= link_to t('.show'), user_photo_album_path(@user, @photo_album), :class => "general" %> |
        <%= link_to t('.back'), user_photo_albums_path(@user), :class => "general" %>
      </div>
    </div>
  </div>
  <div id="sidebar">
    <div class="padding">

    </div>
  </div>
</div>